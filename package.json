{
  "name": "@gcorptools/typeorm-helper",
  "version": "0.3.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcorptools/typeorm-helper.git"
  },
  "keywords": [
    "typeorm",
    "translation",
    "pagination",
    "sorting",
    "filtering"
  ],
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "clean": "yarn del build --force",
    "build": "yarn clean && tsc && tsc-alias -p tsconfig.json",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest",
    "coverage": "jest --watchAll --no-cache --coverage",
    "sonar": "ts-node sonar-scanner.js",
    "format": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "dependencies": {
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "pg-mem": "^2.7.2",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "sonarqube-scanner": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2"
  }
}
